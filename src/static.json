{
    "root": "public/",
    "headers":{
        "*/**":{
            "cache-control": "public, max-age=0, must-revalidate"
        },
        "*/**.js":{
            "cache-control": "public, max-age=31536000, immutable"
        },
        "*/static/**":{
            "cache-control": "public, max-age=31536000, immutable"
        },
        "*/icons/*.png":{
            "cache-control": "public, max-age=31356000, immutable"
        }
    },
    "https_only": true,
    "error_page": "404.html"
}